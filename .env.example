# .env.example
# Ethernaut CTF - Configuration Template
# Copy this file to .env and fill in your values

# Sepolia RPC URL (Alchemy, Infura, or public)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Your wallet private key (NEVER commit the real .env file!)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY

# deployed player contract address
PLAYER_ADDRESS=0xYOUR_PLAYER_ADDRESS

# NotOptimisticPortal Level
NOTOPTIMISTICPORTAL_TARGET=0xYOUR_TARGET_ADDRESS

# Target contract addresses for each Ethernaut level
# CoinFlip Level
COINFLIP_TARGET=0xYOUR_TARGET_ADDRESS

TELEPHONE_TARGET=0xYOUR_TARGET_ADDRESS

TOKEN_TARGET=0xYOUR_TARGET_ADDRESS

# Force Level
FORCE_TARGET=0xYOUR_TARGET_ADDRESS
FORCE_VALUE=1000000000000000  # 0.001 ETH in wei

# Vault Level
VAULT_TARGET=0xYOUR_TARGET_ADDRESS

# King Level
KING_TARGET=0xYOUR_TARGET_ADDRESS

# Reentrancy Level
REENTRANCE_TARGET=0xYOUR_TARGET_ADDRESS

# Preservation Level
PRESERVATION_TARGET=0xYOUR_TARGET_ADDRESS

# AlienCodex Level
ALIENCODEX_TARGET=0xYOUR_TARGET_ADDRESS

# Dex Levels
DEX_TARGET=0xYOUR_TARGET_ADDRESS
DEXTWO_TARGET=0xYOUR_TARGET_ADDRESS

# Shop Level
SHOP_ADDRESS=0xYOUR_TARGET_ADDRESS

PUZZLE_WALLET_ADDRESS=0xYOUR_TARGET_ADDRESS

# Impersonator Level
IMPERSONATOR_ADDRESS=0xYOUR_TARGET_ADDRESS


# Add more level addresses as you progress
# FALLBACK_TARGET=0x...
